<% content_for :back do %>
    <%= link_to courses_path, class: "btn btn-default float-right", :title => (t(:back)) do %>
        <%= fa_icon "reply" %>
  <% end %>
<% end %>

<% content_for :btns do %>
    <%= link_to t(:show_examinations), course_examinations_path(@course), class: "btn btn-primary float-right btn-sm first" %> 
    <%= link_to t(:show_students), course_students_path(@course), class: "btn btn-primary float-right btn-sm first" %> 
<% end %>

<% if !@examinations.empty? && !@students.empty? %>
    <% content_for :table do %>
        <%= render 'resultsTable' %>
    <% end %>
<% end %>

<%= render 'layouts/contentCard', card_title: course_title(@course), collection: nil %>

